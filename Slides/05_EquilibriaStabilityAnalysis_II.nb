}, Open  ]],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", "(", "\[Zeta]", ")"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Zeta]", " ", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox[
           OverscriptBox["n", "^"], "0"], "+", 
          RowBox[{
           SubscriptBox[
            OverscriptBox["n", "^"], "1"], "\[Zeta]"}], "+", 
          RowBox[{
           SubscriptBox[
            OverscriptBox["n", "^"], "2"], 
           SuperscriptBox["\[Zeta]", "2"]}], "+", 
          RowBox[{
           SubscriptBox[
            OverscriptBox["n", "^"], "3"], 
           SuperscriptBox["\[Zeta]", "3"]}], "+", 
          RowBox[{"\[CenterDot]", "\[CenterDot]", "\[CenterDot]"}]}], 
         ")"}]}]], "+", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["n", "^"], "0"], "+", 
        RowBox[{
         SubscriptBox[
          OverscriptBox["n", "^"], "1"], "\[Zeta]"}], "+", 
        RowBox[{
         SubscriptBox[
          OverscriptBox["n", "^"], "2"], 
         SuperscriptBox["\[Zeta]", "2"]}], "+", 
        RowBox[{
         SubscriptBox[
          OverscriptBox["n", "^"], "3"], 
         SuperscriptBox["\[Zeta]", "3"]}], "+", 
        RowBox[{"\[CenterDot]", "\[CenterDot]", "\[CenterDot]"}]}], ")"}], 
      "-", "5"}], "=", "0."}]}]}], TraditionalForm]], "DisplayFormulaNumbered",\

 CellChangeTimes->{{3.639880980278393*^9, 3.639881028734737*^9}, {
  3.639881153313669*^9, 3.639881292632408*^9}, {3.6399137281394978`*^9, 
  3.6399137378197803`*^9}}],

Cell[TextData[{
 "(3) The first three derivatives in the Taylor series in Eq. (2), evaluated \
at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Zeta]", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", are ",
 Cell[BoxData[
