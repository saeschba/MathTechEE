Cell[CellGroupData[{

Cell["Problems", "Section",
 CellChangeTimes->{{3.639273060208815*^9, 3.639273061503702*^9}}],

Cell["\<\
[Problem 5.14 in OD2007] Consider a variant of the logistic model with \
immigration, as given by the following recursion equation:\
\>", "ItemNumbered",
 CellChangeTimes->{{3.6392737799779453`*^9, 3.63927381459451*^9}, {
  3.639273849696615*^9, 3.6392738591183662`*^9}, {3.6399655972853603`*^9, 
  3.639965623499488*^9}, {3.639965669193314*^9, 3.6399656719119663`*^9}, {
  3.639965753460313*^9, 3.639965753891078*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"n", 
     RowBox[{"(", 
      RowBox[{"t", "+", "1"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", "r"}], ")"}], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[Alpha]"}], " ", "n", 
        RowBox[{"(", "t", ")"}]}]], "n", 
      RowBox[{"(", "t", ")"}]}], "+", "m"}]}]}], 
