Cell[CellGroupData[{

Cell[TextData[{
 "(4) For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "0", ")"}], "=", 
    RowBox[{
     RowBox[{
      OverscriptBox["n", "^"], "-", "4"}], "=", "0"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["n", "^"], "0"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", which gives ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["n", "^"], "0"], "=", "4"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". Proceeding with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["n", "^"], "0"], "+", 
     SubscriptBox[
      OverscriptBox["n", "^"], "1"]}], "=", 
    RowBox[{
     RowBox[{"4", "+", 
      SubscriptBox[
       OverscriptBox["n", "^"], "1"]}], "=", "0"}]}], TraditionalForm]]],
 " for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["n", "^"], "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", which gives ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["n", "^"], "1"], "=", 
    RowBox[{"-", "4"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "=", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox[
