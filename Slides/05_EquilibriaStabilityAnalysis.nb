Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"p", " ", 
     RowBox[{"(", 
      RowBox[{"t", "+", "1"}], ")"}]}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
