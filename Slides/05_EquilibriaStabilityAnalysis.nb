  3.639311308462287*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    OverscriptBox["p", "^"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{
        SuperscriptBox[
         OverscriptBox["p", "^"], "2"], 
        SubscriptBox["W", 
         StyleBox["AA",
          FontSlant->"Italic"]]}], "+", 
