         StyleBox["AA",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{
        RowBox[{"p", "(", "t", ")"}], 
        RowBox[{"q", "(", "t", ")"}], 
        SubscriptBox["W", 
         StyleBox["Aa",
          FontSlant->"Italic"]]}]}], 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"p", "(", "t", ")"}], "2"], 
        SubscriptBox["W", 
         StyleBox["AA",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{"2", 
        RowBox[{"p", "(", "t", ")"}], 
        RowBox[{"q", "(", "t", ")"}], 
        SubscriptBox["W", 
         StyleBox["Aa",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"q", "(", "t", ")"}], "2"], 
        SubscriptBox["W", 
         StyleBox["aa",
          FontSlant->"Italic"]]}]}]], "."}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.63930929877497*^9, 3.6393093207998257`*^9}, {
  3.639309356983395*^9, 3.639309361104203*^9}, {3.639310745369378*^9, 
  3.639310782142582*^9}, {3.6393110513349857`*^9, 3.639311124257414*^9}, {
  3.63931128227966*^9, 3.639311323620232*^9}}],

Cell[TextData[{
 "Note that before proceeding, we have to express ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "-", 
    RowBox[{"p", "(", "t", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " here. It would be wrong to leave ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " in there and treat it as constant; it is a function of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "! The equilibrium condition therefore becomes"
}], "Text",
 CellChangeTimes->{{3.639311129062819*^9, 3.639311169223948*^9}, {
  3.63931123786976*^9, 3.639311260988373*^9}, {3.6393112926697397`*^9, 
  3.639311308462287*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    OverscriptBox["p", "^"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{
        SuperscriptBox[
         OverscriptBox["p", "^"], "2"], 
        SubscriptBox["W", 
         StyleBox["AA",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{
        RowBox[{
         OverscriptBox["p", "^"], "(", 
         RowBox[{"1", "-", 
          OverscriptBox["p", "^"]}], ")"}], 
        SubscriptBox["W", 
         StyleBox["Aa",
          FontSlant->"Italic"]]}]}], 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         OverscriptBox["p", "^"], "2"], 
        SubscriptBox["W", 
         StyleBox["AA",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{"2", 
        RowBox[{
         OverscriptBox["p", "^"], "(", 
         RowBox[{"1", "-", 
          OverscriptBox["p", "^"]}], ")"}], 
        SubscriptBox["W", 
         StyleBox["Aa",
          FontSlant->"Italic"]]}], "+", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "-", 
           OverscriptBox["p", "^"]}], ")"}], "2"], 
        SubscriptBox["W", 
         StyleBox["aa",
          FontSlant->"Italic"]]}]}]], "."}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.63930929877497*^9, 3.6393093207998257`*^9}, {
  3.639309356983395*^9, 3.639309361104203*^9}, {3.639310745369378*^9, 
  3.639310782142582*^9}, {3.6393110513349857`*^9, 3.639311124257414*^9}, {
  3.639311271831658*^9, 3.6393112765422373`*^9}, {3.6393113276896143`*^9, 
  3.639311359415402*^9}}],

Cell[TextData[{
 "Solving for ",
 Cell[BoxData[
