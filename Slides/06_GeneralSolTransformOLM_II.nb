Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    FractionBox[
     RowBox[{"\[DifferentialD]", "\[Delta]"}], 
     RowBox[{"\[DifferentialD]", "t"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[DifferentialD]", 
       RowBox[{"(", 
