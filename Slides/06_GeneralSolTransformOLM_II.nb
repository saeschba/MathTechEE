      RowBox[{"\[DifferentialD]", "t"}]], "=", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"\[DifferentialD]", "n"}], 
        RowBox[{"\[DifferentialD]", "t"}]], "-", 
       FractionBox[
        RowBox[{"\[DifferentialD]", 
         OverscriptBox["n", "^"]}], 
        RowBox[{"\[DifferentialD]", "t"}]]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"\[Rho]", " ", "n"}], "+", "c"}], " ", "=", 
       "\[LineSeparator]", "\t", 
       RowBox[{
