Cell[CellGroupData[{

Cell["Expressing a function as a power series", "Subsection",
 CellChangeTimes->{{3.638185844879794*^9, 3.6381858498618307`*^9}, {
  3.638186778158493*^9, 3.638186783643787*^9}}],

Cell["Most functions can be rewritten as the infinite series", "Text",
 CellChangeTimes->{{3.638185713261051*^9, 3.638185794855603*^9}, {
  3.638185854670094*^9, 3.638185856230301*^9}, {3.6381859253832197`*^9, 
  3.638185927335332*^9}, {3.6381867906543427`*^9, 3.638186806173802*^9}, {
  3.6381868369587393`*^9, 3.6381868385989313`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", "0"], "+", 
      RowBox[{
       SubscriptBox["b", "1"], "x"}], "+", 
      RowBox[{
       SubscriptBox["b", "2"], 
       SuperscriptBox["x", "2"]}], "+"}], "..."}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.6381868181598377`*^9, 3.63818683232831*^9}, {
  3.638186899704413*^9, 3.638186902233748*^9}}],

Cell[TextData[{
 "This infinite series is referred to as a ",
 StyleBox["power series",
  FontWeight->"Bold"],
 ", because it contains ",
 StyleBox["terms that are integer powers",
  FontSlant->"Italic"],
 " of ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.6381859292630444`*^9, 3.638185942206683*^9}, {
  3.638186171540996*^9, 3.638186193033475*^9}, {3.6381868470075893`*^9, 
  3.6381868702078247`*^9}}],

Cell[TextData[{
 "Suppose we want to focus on the behaviour of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " around a particular point ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". This corresponds to ",
 StyleBox["shifting the power series along the ",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSlant->"Plain"],
 StyleBox[" axis",
  FontSlant->"Italic"],
 " by an amount ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". From Rule P1.1, we therefore have"
}], "Text",
 CellChangeTimes->{{3.638186913328952*^9, 3.638186949271996*^9}, {
  3.638187004482245*^9, 3.638187053905878*^9}, {3.638187093415538*^9, 
  3.638187136866733*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", "0"], "+", 
      RowBox[{
       SubscriptBox["b", "1"], "(", 
       RowBox[{"x", "-", "a"}], ")"}], "+", 
      SuperscriptBox[
       RowBox[{
        SubscriptBox["b", "2"], "(", 
        RowBox[{"x", "-", "a"}], ")"}], "2"], "+"}], "..."}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.638187060540168*^9, 3.6381870748384447`*^9}, {
  3.638187264126051*^9, 3.638187276278656*^9}}],

Cell[TextData[{
 "This is referred to as \[OpenCurlyDoubleQuote]a power series in ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", around the point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\[CloseCurlyDoubleQuote]."
}], "Text",
 CellChangeTimes->{{3.6381872911059217`*^9, 3.638187312594975*^9}}],

Cell[TextData[{
 "All that is left to do is to find the coefficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". The Taylor series does just that..."
}], "Text",
 CellChangeTimes->{{3.638187364406563*^9, 3.638187400189746*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["", "SlideShowNavigationBar",
