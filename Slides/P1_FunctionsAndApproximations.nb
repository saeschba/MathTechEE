Cell[CellGroupData[{

Cell["Definition P1.9: Series", "Subsection",
 CellChangeTimes->{{3.638183297256875*^9, 3.63818331768531*^9}, {
  3.638185145731304*^9, 3.638185150198378*^9}}],

Cell[TextData[{
 "A ",
 StyleBox["series",
  FontWeight->"Bold"],
 " is a ",
 StyleBox["sum of the terms of a sequence",
  FontSlant->"Italic"],
 " (e.g. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      SubscriptBox["a", "1"], "+", 
      SubscriptBox["a", "2"], "+"}], "..."}], "+", 
    SubscriptBox["a", "k"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ").\nExamples:"
}], "Text",
 CellChangeTimes->{{3.638183319294306*^9, 3.63818333589403*^9}, {
  3.638183396192616*^9, 3.638183457810326*^9}, {3.638185158606947*^9, 
  3.638185211809268*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "(i) Series of squared natural numbers: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"0", "+", "1", "+", "4", "+", "9", "+"}], "..."}], "+", 
     SuperscriptBox["k", "2"]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"a", "=", "0"}], "k"], 
     SuperscriptBox["a", "2"]}]}], TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.638183478250358*^9, 3.6381835182710257`*^9}, {
  3.63818502410632*^9, 3.638185026650387*^9}, {3.638185244169676*^9, 
  3.6381853305077353`*^9}, {3.638185412032496*^9, 3.638185412032546*^9}, {
  3.6381951145694103`*^9, 3.6381951211612883`*^9}},
 FontColor->RGBColor[0., 0.7091783016708629, 0.35761043717097735`]],

Cell[TextData[{
 "(ii) Geometric series: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SuperscriptBox["2", "0"]], "+", 
       FractionBox["1", 
        SuperscriptBox["2", "1"]], "+", 
       FractionBox["1", 
        SuperscriptBox["2", "2"]], "+", 
       FractionBox["1", 
        SuperscriptBox["2", "3"]], "+"}], "..."}], "+", 
     FractionBox["1", 
      SuperscriptBox["2", "k"]]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "o"}], "k"], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["1", "2"], ")"}], "i"]}]}], TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.638183478250358*^9, 3.638183524789452*^9}, {
  3.638185030602695*^9, 3.6381851213228292`*^9}, {3.6381853569346867`*^9, 
  3.638185406698421*^9}, {3.6381854372748623`*^9, 3.638185444306855*^9}, {
  3.6381860881378717`*^9, 3.638186098202018*^9}, {3.638195124193306*^9, 
  3.638195127057333*^9}},
 FontColor->RGBColor[0., 0.7091783016708629, 0.35761043717097735`]]
}, Open  ]],

Cell[TextData[{
 "If there are an ",
 StyleBox["infinite number of terms",
  FontSlant->"Italic"],
 ", we say it is an ",
 StyleBox["infinite series",
  FontWeight->"Bold"],
 ", and represent it as"
}], "Text",
 CellChangeTimes->{{3.638185452803546*^9, 3.638185500753778*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{
     SubscriptBox["a", "0"], "+", 
     SubscriptBox["a", "1"], "+", 
     SubscriptBox["a", "2"], "+"}], "..."}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.63818550789986*^9, 3.638185518388055*^9}, {
  3.638185553878396*^9, 3.638185569598568*^9}}],

