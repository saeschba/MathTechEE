Cell[CellGroupData[{

Cell["Rule P1.1: Changing the shape of a function", "Subsection",
 CellChangeTimes->{{3.6380996106743183`*^9, 3.6380996300987463`*^9}}],

Cell[TextData[{
 "The following rules can be used to alter a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " to match a desired shape. This can be very useful in practice!"
}], "Text",
 CellChangeTimes->{{3.638099637863009*^9, 3.638099677576041*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "To ",
 StyleBox["shift a function to the right or left",
  FontWeight->"Bold"],
 " by an amount of ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", replace ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"x", "-", "d"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or (",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "+", "d"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "), respectively"
}], "Item",
 CellChangeTimes->{{3.6380996979069242`*^9, 3.63809979197444*^9}}],

Cell[TextData[{
 "To ",
 StyleBox["increase or decrease the height of a function",
  FontWeight->"Bold"],
 " by an amount of ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", replace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "+", "d"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "-", "d"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", respectively"
}], "Item",
 CellChangeTimes->{{3.6380996979069242`*^9, 3.638099854063939*^9}}],

Cell[TextData[{
 "To ",
 StyleBox["scale a function along the horizontal (",
  FontWeight->"Bold"],
 StyleBox["x",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[") axis",
  FontWeight->"Bold"],
 " by a factor ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", replace ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "/", "d"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". ",
 StyleBox["Stretching",
  FontSlant->"Italic"],
 " occurs if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", ">", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", ",
 StyleBox["shrinking",
  FontSlant->"Italic"],
 " occurs if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.6380996979069242`*^9, 3.6380998947226543`*^9}, {
  3.6380999891223383`*^9, 3.638099997594137*^9}, {3.638100206051297*^9, 
  3.638100232966752*^9}}],

Cell[TextData[{
 "To ",
 StyleBox["scale a function along the vertical (",
  FontWeight->"Bold"],
 StyleBox["y",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[") axis",
  FontWeight->"Bold"],
 " by a factor of ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", replace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", " ", 
    RowBox[{"f", "(", "x", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". ",
 StyleBox["Stretching",
  FontSlant->"Italic"],
 " occurs if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", ">", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", ",
 StyleBox["shrinking",
  FontSlant->"Italic"],
 " occurs if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "<", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.6380996979069242`*^9, 3.638099933129479*^9}, {
  3.638100259560595*^9, 3.6381003001969347`*^9}, {3.638100343392741*^9, 
  3.6381003444964237`*^9}}],

Cell[TextData[{
 "To ",
 StyleBox["reflect a function across the horizontal (",
  FontWeight->"Bold"],
 StyleBox["x",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[") axis",
  FontWeight->"Bold"],
 ", replace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    RowBox[{"f", "(", "x", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Item",
 CellChangeTimes->{{3.6380996979069242`*^9, 3.638099933129479*^9}, {
  3.638100259560595*^9, 3.6381003589346457`*^9}}]
}, Open  ]],

