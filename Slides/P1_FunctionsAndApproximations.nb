      RowBox[{"(", 
       RowBox[{"more", " ", 
        RowBox[{"precisely", ":", " ", 
         RowBox[{
          SubscriptBox["lim", 
           RowBox[{"n", "\[Rule]", "\[Infinity]"}]], 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
