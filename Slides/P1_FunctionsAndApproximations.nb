Cell[CellGroupData[{

Cell[TextData[{
 "Recipe P1.2 Obtaining the Taylor series approximation of a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Subsection",
 CellChangeTimes->{{3.6381894098171253`*^9, 3.63818943301779*^9}}],

Cell[TextData[{
 "Most functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " can be approximated around the point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " by  truncating the Taylor series to the degree of accuray desired and \
ignoring remaining terms. If the terms that are ignored are on the order of \
",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"x", "-", "a"}], ")"}], "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we write these terms as ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"O", "(", 
     RowBox[{"x", "-", "a"}], ")"}], "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". Doing so clarifies the degree of accuracy of the approximation."
}], "Text",
 CellChangeTimes->{{3.638199114231029*^9, 3.638199215027834*^9}}],

Cell[TextData[{
 "The crudest approximation is the ",
 StyleBox["constant approximation",
  FontWeight->"Bold"],
 ":"
}], "Text",
 CellChangeTimes->{{3.638199237363347*^9, 3.6381992440113897`*^9}, {
  3.6381995432563972`*^9, 3.638199545008725*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", "a", ")"}], "+", 
     RowBox[{"O", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"x", "-", "a"}], ")"}], "."}]}]}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.6381992556040173`*^9, 3.638199299609212*^9}, 
   3.6381993968989477`*^9, {3.638199432041809*^9, 3.638199433674231*^9}}],

Cell[TextData[{
 "It is correct only at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and provides no information about what happens as we move ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " even just a little away from ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.638199314634673*^9, 3.638199355618134*^9}}],

Cell[TextData[{
 "A more accuracte approximation is the line, i.e. a ",
 StyleBox["linear approximation",
  FontWeight->"Bold"],
 ":"
}], "Text",
 CellChangeTimes->{{3.63819935888225*^9, 3.638199376522215*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", "a", ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", "f"}], 
         RowBox[{"\[DifferentialD]", "x"}]], 
        SubscriptBox["|", 
         RowBox[{"x", "=", "a"}]]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"x", "-", "a"}], ")"}]}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"O", "(", 
        RowBox[{"x", "-", "a"}], ")"}], "2"], "."}]}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.6381992556040173`*^9, 3.638199299609212*^9}, {
  3.638199386428789*^9, 3.638199440329941*^9}}],

Cell[TextData[{
 "The term ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[DifferentialD]", "f"}], "/", 
     RowBox[{"\[DifferentialD]", "x"}]}], 
    SubscriptBox["|", 
     RowBox[{"x", "=", "a"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the slope and determines whether the function increases or decreases as \
we move ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " away from ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.638199456633296*^9, 3.6381995198166237`*^9}}],

Cell["", "PageBreak",
 PageBreakBelow->True],

Cell[TextData[{
 "Including the next order gives a ",
 StyleBox["quadratic approximation",
  FontWeight->"Bold"],
 ":"
}], "Text",
 CellChangeTimes->{{3.638199523400558*^9, 3.638199559520298*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", "a", ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", "f"}], 
         RowBox[{"\[DifferentialD]", "x"}]], 
        SubscriptBox["|", 
         RowBox[{"x", "=", "a"}]]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"x", "-", "a"}], ")"}]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["\[DifferentialD]", "2"], "f"}], 
         RowBox[{"\[DifferentialD]", 
          SuperscriptBox["x", "2"]}]], 
        SubscriptBox["|", 
         RowBox[{"x", "=", "a"}]]}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", "-", "a"}], ")"}], "2"]}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"O", "(", 
        RowBox[{"x", "-", "a"}], ")"}], "3"], "."}]}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.6381992556040173`*^9, 3.638199299609212*^9}, {
  3.638199386428789*^9, 3.638199440329941*^9}, {3.638199578745901*^9, 
  3.638199610913835*^9}}],

Cell[TextData[{
 "The approximation implies that the function curves upward around ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " when ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[DifferentialD]", "2"], "f"}], "/", 
     RowBox[{"\[DifferentialD]", 
      SuperscriptBox["x", "2"]}]}], 
    SubscriptBox["|", 
     RowBox[{"x", "=", "a"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is positive (convex), but downward if it is negative (concave)."
}], "Text",
 CellChangeTimes->{{3.638199624359847*^9, 3.638199706231204*^9}}],

Cell[TextData[{
 "In practice, we often ignore the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"O", "(", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " terms, but replace the \[OpenCurlyDoubleQuote]equals\
\[CloseCurlyDoubleQuote] sign by an \[OpenCurlyDoubleQuote]approximately \
equals\[CloseCurlyDoubleQuote] sign, i.e."
}], "Text",
 CellChangeTimes->{{3.638199720408374*^9, 3.638199784126604*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", "a", ")"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"\[DifferentialD]", "f"}], 
         RowBox[{"\[DifferentialD]", "x"}]], 
        SubscriptBox["|", 
         RowBox[{"x", "=", "a"}]]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"x", "-", "a"}], ")"}]}], "+", 
     SuperscriptBox[
      RowBox[{"O", "(", 
       RowBox[{"x", "-", "a"}], ")"}], "2"]}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.6381992556040173`*^9, 3.638199299609212*^9}, {
   3.638199386428789*^9, 3.638199440329941*^9}, 3.6381998129582*^9}],

Cell["can be written as", "Text",
 CellChangeTimes->{{3.638199815726087*^9, 3.638199819966257*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], "\[TildeTilde]", 
     RowBox[{
      RowBox[{"f", "(", "a", ")"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", "f"}], 
          RowBox[{"\[DifferentialD]", "x"}]], 
         SubscriptBox["|", 
          RowBox[{"x", "=", "a"}]]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"x", "-", "a"}], ")"}]}]}]}], ","}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.6381992556040173`*^9, 3.638199299609212*^9}, {
  3.638199386428789*^9, 3.638199440329941*^9}, {3.638199825678584*^9, 
  3.638199826151494*^9}, {3.6381998650619183`*^9, 3.638199873541614*^9}}],

Cell["\<\
which is shorter, but it may not be immediately clear of what order the \
approximation is.\
\>", "Text",
 CellChangeTimes->{{3.638199829659349*^9, 3.6381998590219183`*^9}}]
}, Open  ]]
}, Open  ]],

