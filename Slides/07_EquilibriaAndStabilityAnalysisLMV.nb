Cell["\<\
Example: Metastasis of malignant tumors (adapted from Glass and Kaplan 1995)\
\>", "Subsubsection",
 CellChangeTimes->{{3.6412363072936563`*^9, 3.641236314120348*^9}, {
  3.641236777487678*^9, 3.641236802815053*^9}}],

Cell[TextData[{
 "Concider the case of a cancer forming metastases. Let the number of cancer \
cells in the capillaries of an organ be ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", and the number of cancer cells that have actually invaded the organ be ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.641262509448834*^9, 3.641262641334489*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "Suppose that cells are lost from the capillaries by death at a per capita \
rate ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Delta]", "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.641262654254424*^9, 3.6412627094238157`*^9}}],

Cell[TextData[{
 "Assume that cells from the capillaries invade the organ at a per capita \
rate ",
 Cell[BoxData[
  FormBox["\[Beta]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.641262654254424*^9, 3.641262747822422*^9}}],

Cell[TextData[{
 "Let the per capita rate of death of cells in the organ be ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Delta]", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.641262654254424*^9, 3.641262782622006*^9}}],

Cell[TextData[{
 "Last, assume that cancer cells that have invaded the organ replicate at a \
per capita rate ",
 Cell[BoxData[
  FormBox["\[Rho]", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Item",
 CellChangeTimes->{{3.641262654254424*^9, 3.6412628157248373`*^9}, {
  3.641262862372635*^9, 3.641262863036317*^9}}]
}, Open  ]],

