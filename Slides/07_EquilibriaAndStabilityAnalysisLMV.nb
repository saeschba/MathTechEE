Cell[CellGroupData[{

Cell["\<\
Equilibria and stability for linear multivariable models in discrete time\
\>", "Section",
 CellChangeTimes->{{3.641236267048511*^9, 3.641236289274625*^9}}],

Cell["\<\
A linear multivariable model in discrete time can be expressed as a recursion \
equation in matrix form as follows:\
\>", "Text",
 CellChangeTimes->{{3.641291152373352*^9, 3.641291185778768*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["n", "\[RightVector]"], "(", 
      RowBox[{"t", "+", "1"}], ")"}], "=", 
     RowBox[{
      StyleBox["M",
       FontWeight->"Bold",
       FontSlant->"Plain"], " ", 
      RowBox[{
       OverscriptBox["n", "\[RightVector]"], "(", "t", ")"}]}]}], ","}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.64068828890628*^9, 3.6406883414825907`*^9}, {
  3.641032801997142*^9, 3.6410328272748537`*^9}, {3.64103368445313*^9, 
  3.641033717770136*^9}, {3.6412911899217167`*^9, 3.64129119049574*^9}}],

Cell["where, in the case of just two variables,", "Text",
 CellChangeTimes->{{3.641291193162423*^9, 3.641291212354313*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["n", "\[RightVector]"], "(", 
      RowBox[{"t", "+", "1"}], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          SubscriptBox["n", "1"], "(", 
          RowBox[{"t", "+", "1"}], ")"}]},
        {
         RowBox[{
          SubscriptBox["n", "2"], "(", 
          RowBox[{"t", "+", "1"}], ")"}]}
       }], ")"}]}], ","}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.64068828890628*^9, 3.6406883414825907`*^9}, {
  3.641032801997142*^9, 3.6410328272748537`*^9}, {3.64103368445313*^9, 
  3.641033717770136*^9}, {3.6412911899217167`*^9, 3.64129119049574*^9}, {
  3.641291222002675*^9, 3.641291234235355*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["n", "\[RightVector]"], "(", "t", ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          SubscriptBox["n", "1"], "(", "t", ")"}]},
        {
         RowBox[{
          SubscriptBox["n", "2"], "(", "t", ")"}]}
       }], ")"}]}], ",", " ", "and"}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.64068828890628*^9, 3.6406883414825907`*^9}, {
  3.641032801997142*^9, 3.6410328272748537`*^9}, {3.64103368445313*^9, 
  3.641033717770136*^9}, {3.6412911899217167`*^9, 3.64129119049574*^9}, {
  3.6412912412474937`*^9, 3.64129125918535*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{
    StyleBox["M",
     FontWeight->"Bold",
     FontSlant->"Plain"], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"a", "b"},
        {"c", "d"}
       }], ")"}], "."}]}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.64068828890628*^9, 3.6406883414825907`*^9}, {
  3.641032801997142*^9, 3.6410328272748537`*^9}, {3.64103368445313*^9, 
  3.641033717770136*^9}, {3.6412911899217167`*^9, 3.64129119049574*^9}, {
  3.641291262751461*^9, 3.6412912781708803`*^9}}],

Cell[TextData[{
 "The matrix ",
 Cell[BoxData[
  FormBox[
   StyleBox["M",
    FontWeight->"Bold",
    FontSlant->"Plain"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is sometimes called the ",
 StyleBox["transition matrix",
  FontSlant->"Italic"],
 ", as it describes the transition from one to the next time step."
}], "Text",
 CellChangeTimes->{{3.641291280616681*^9, 3.6412913237762403`*^9}}]
}, Open  ]]
}, Open  ]],

